{
  "name": "sms-ui",
  "version": "1.0.0",
  "description": "User interface of GOYOTASHI2020",
  "main": "main.js",
  "scripts": {
    "start": "electron . --force --goyodebug",
    "package": "node ./scripts/packaging.js",
    "signInstallerBuild": "node ./scripts/signInstallerBuild.js",
    "wintest": "electron wintest.js window",
    "testBS": "electron editBookrackSetting.js",
    "dialogtest": "electron wintest.js dialog",
    "test": "electron --inspect-brk . --force --goyodebug",
    "pcenv": "electron . --env",
    "smanager": "electron . --smanager",
    "build": "npm run build:goyop && npm run build:goyolib",
    "build:goyop": "protocol-embeditor --obfuscate --libname=goyop --protocol=goyop --out ./lib --contentsdir ./contents --builddir _build/goyop --gyp_runtime=electron --gyp_target=2.0.7 --gyp_disturl=https://atom.io/download/atom-shell",
    "build:goyolib": "native-embeditor --obfuscate --name=goyolib --out ./lib --contentsdir ./lib --packages goyo-bookrack-accessor,old-goyo-bookrack-accessor,photo-metadata-accessor,goyo-migration,goyo-deliverable-data --builddir _build/goyolib --gyp_runtime=electron --gyp_target=2.0.7 --gyp_disturl=https://atom.io/download/atom-shell",
    "makeregistry": "powershell.exe -ExecutionPolicy RemoteSigned -File .\\scripts\\make-registry-entry.ps1",
    "removeregistry": "powershell.exe -ExecutionPolicy RemoteSigned -File .\\scripts\\remove-registry-entry.ps1",
    "licenselist": "node ./scripts/check-licenses.js"
  },
  "author": "SALTYSTER",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "adm-zip": "^0.4.11",
    "byline": "^5.0.0",
    "commander": "^2.15.1",
    "dateformat": "^3.0.3",
    "del": "=3.0.0",
    "dirsize": "^2.0.0",
    "electron-dialogbox": "file:../vendor/electron-dialogbox",
    "encoding-japanese": "^1.0.29",
    "filesize": "^3.6.1",
    "find-process": "^1.2.1",
    "font-manager": "^0.3.0",
    "get-folder-size": "^2.0.0",
    "sms-accessor": "../sms-accessor",
    "jimp": "=0.5.6",
    "lodash.clonedeep": "=4.5.0",
    "node-wmic": "=1.1.3",
    "quill-delta-to-html": "=0.8.4",
    "rage-edit": "=1.2.0",
    "ref": "=1.3.5",
    "regedit": "=3.0.0",
    "robotjs": "=0.5.1",
    "systeminformation": "=3.47.0",
    "universalify": "=0.1.2",
    "xlsx": "=0.14.1",
    "xlsx-populate": "=1.19.0",
    "xml2js": "=0.4.19",
    "imagesloaded": "=4.1.4",
    "escape-html": "=1.0.3"
  },
  "devDependencies": {
    "asar": "=0.14.3",
    "electron": "../vendor/electron",
    "electron-ava": "^0.1.0",
    "fs-extra": "^7.0.0",
    "innosetup-compiler": "^5.5.9",
    "javascript-obfuscator": "^0.18.1",
    "license-checker": "^19.0.0",
    "prompts": "^1.2.1",
    "rcedit": "^1.1.0",
    "rewire": "=4.0.1",
    "signtool": "../vendor/signtool",
    "wget-improved": "^3.0.1"
  }
}
